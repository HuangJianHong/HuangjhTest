<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgress">
        <attr name="max" format="integer" />
        <attr name="round_width" format="dimension" />
        <attr name="round_color" format="color" />
        <attr name="progress_color" format="color" />
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="style">
            <enum name="STROKE" value="1" />
            <enum name="FILL" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

</resources>